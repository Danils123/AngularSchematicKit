{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "A blank schematic.",
      "factory": "./ng-add/index",
      "schema": "./ng-add/schema.json"
    },
    "initialize-angular-configuration": {
      "description": "A blank schematic.",
      "factory": "./ng-generate/configureProject/index#configureProject",
      "schema": "./ng-generate/configureProject/schema.json",
      "aliases": [
        "start"
      ]
    },
    "scan": {
      "description": "Scan project and represent into a json file",
      "factory": "./ng-generate/scan-project/index#scan",
      "schema": "./ng-generate/scan-project/schema.json",
      "aliases": [
        "s"
      ]
    },
    "build": {
      "description": "Execute all the schematics in the json file",
      "factory": "./ng-generate/build/index#buildJSON",
      "schema": "./ng-generate/build/schema.json",
      "aliases": [
        "b"
      ]
    },
    "scaffolding": {
      "description": "recreate scaffold",
      "factory": "./ng-generate/scaffolding/index#scaffolding",
      "schema": "./ng-generate/scaffolding/schema.json",
      "aliases": [
        "s"
      ]
    },
    "customScaffold": {
      "description": "recreate custom scaffold",
      "factory": "./ng-generate/scaffolding/custom-scaffolding/index#customScaffolding",
      "schema": "./ng-generate/scaffolding/custom-scaffolding/schema.json"
    },
    "add-linters": {
      "description": "Add default linters",
      "factory": "./ng-generate/linters/index#addLinters",
      "schema": "./ng-generate/linters/schema.json",
      "aliases": [
        "l"
      ]
    },
    "add-git-hooks": {
      "description": "Add Git-hooks with Husky",
      "factory": "./ng-generate/gitHooks/index#addGitHooks",
      "schema": "./ng-generate/gitHooks/schema.json",
      "aliases": [
        "gh"
      ]
    },
    "newFeature": {
      "description": "Add a new scaffold feature",
      "factory": "./ng-generate/new-feature-structure/index",
      "schema": "./ng-generate/new-feature-structure/schema.json"
    }
  }
}