{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "utils": [
        "src/utils/utils.ts"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ],
    "target": "ES2021",
    "module": "CommonJS",
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": false,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "tsBuildInfoFile": "./dist/tsbuildinfo",
    "types": [
      "node"
    ]
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./src/*/files/**/*"
  ]
}
